{
  "name": "vite-plugin-nx-dotenv",
  "version": "1.0.1",
  "description": "Support Vite's [mode] mode of dotenv usage in Nx",
  "keywords": [
    "vite",
    "vite-plugin",
    "vue",
    "nx",
    "dotenv"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:types": "rm -rf build && tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "docs": "rm -rf docs && typedoc src --out docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chf007/vite-plugin-nx-dotenv.git"
  },
  "author": "chf007server@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chf007/vite-plugin-nx-dotenv/issues"
  },
  "homepage": "https://github.com/chf007/vite-plugin-nx-dotenv#readme",
  "peerDependencies": {
    "vite": ">=2.9.4"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.23.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "eslint": "^8.14.0",
    "rollup": "^2.70.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
